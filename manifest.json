{
    "manifest_version": 3,
    "name": "Safety 550",
    "description": "Google Chrome Online Safety Extension",
    "version": "1.0",
    "permissions": ["webNavigation", "storage", "tabs"],
    "action": {
      "default_icon": "images/light-bulb.jpg",
      "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    }
  }